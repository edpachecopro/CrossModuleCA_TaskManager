{
  "name": "electron-packager-tf",
  "version": "7.3.3",
  "description": "Package and distribute your Electron app with OS-specific bundles (.app, .exe etc) via JS or CLI",
  "main": "index.js",
  "bin": {
    "electron-packager": "cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electron-userland/electron-packager.git"
  },
  "author": "max ogden",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/electron-userland/electron-packager/issues"
  },
  "homepage": "https://github.com/electron-userland/electron-packager",
  "dependencies": {
    "asar": "^0.11.0",
    "bluebird": "^3.4.0",
    "electron-download": "^2.1.2",
    "electron-osx-sign": "^0.4.0-beta4",
    "extract-zip": "^1.5.0",
    "fs-extra": "^0.30.0",
    "fs-extra-p": "^1.0.2",
    "get-package-info": "0.0.2",
    "minimist": "^1.2.0",
    "plist": "^1.2.0",
    "rcedit": "^0.5.0",
    "resolve": "^1.1.7",
    "run-series": "^1.1.4",
    "sanitize-filename": "^1.6.0"
  },
  "devDependencies": {
    "buffer-equal": "^1.0.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.12.0",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-standard": "^1.3.2",
    "is-admin": "^2.0.0",
    "nyc": "^6.6.1",
    "rcinfo": "^0.1.3",
    "rimraf": "^2.5.2",
    "run-waterfall": "^1.1.3",
    "tape": "^4.5.1"
  },
  "engines": {
    "node": ">= 4.0"
  },
  "scripts": {
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "pretest": "rimraf test/work",
    "test": "eslint . && nyc --cache tape test"
  },
  "directories": {
    "test": "test"
  },
  "keywords": [],
  "eslintConfig": {
    "extends": "standard",
    "parserOptions": {
      "sourceType": "script"
    },
    "rules": {
      "strict": [
        "error"
      ]
    }
  }
}
